<a
	href="{{ href }}"
	class="{{ styleModifier }}"
	{{# btnRole }}
		role="{{ btnRole }}"
	{{/ btnRole }}
	{{# onClick }}
		onClick="{{ onClick }}"
	{{/ onClick }}
	{{ attribute }}
	{{# ariaLabel }}
			aria-label="{{ ariaLabel }}"
	{{/ ariaLabel }}
	{{# ariaPressed }}
		aria-pressed="{{ ariaPressed }}"
	{{/ ariaPressed }}
	{{# tabindex }}
		tabindex="{{tabindex}}"
	{{/ tabindex }}	>
	{{# iconClass }}
		<i class="{{ iconClass }}"></i>
	{{/ iconClass }}
	{{contextualText}}
</a>